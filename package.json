{
  "name": "tv-show-rss-api",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build && pm2 start ./bin/www",
    "prod": "npm run build && node ./bin/www",
    "dev": "nodemon --exec babel-node ./bin/www",
    "lint": "eslint **/*.js --ignore-path .gitignore --ignore-pattern internals/scripts || exit 0 || true",
    "cron": "babel-node ./cron.js",
    "build": "babel src -d dist",
    "deploy": "git push digitalocean master",
    "log:morgan": "node read.js morgan",
    "log:cron": "node read.js cron"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "body-parser": "~1.17.1",
    "compression": "^1.6.2",
    "debug": "~2.6.3",
    "dotenv-safe": "^4.0.3",
    "express": "~4.15.2",
    "lodash": "^4.17.4",
    "mongoose": "^4.9.5",
    "morgan": "~1.8.1",
    "moviedb": "^0.2.8",
    "omdb": "^0.8.0",
    "pm2": "^2.4.5",
    "rarbg": "^0.4.4",
    "rss": "^1.2.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "nodemon": "^1.11.0"
  },
  "engines": {
    "node": "7.6.0"
  }
}
