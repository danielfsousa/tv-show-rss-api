{
  "name": "tv-show-rss-api",
  "version": "2.1.2",
  "private": true,
  "scripts": {
    "start": "npm run build && pm2 start ./bin/www",
    "test": "(set NODE_ENV=test) & (mocha --timeout 120000 --require babel-register src/**/*.test.js)",
    "prod": "npm run build && node ./bin/www",
    "dev": "nodemon --exec babel-node ./bin/www",
    "lint": "eslint **/*.js --ignore-path .gitignore --ignore-pattern internals/scripts || exit 0 || true",
    "cron": "babel-node ./src/cron.js",
    "build": "babel src -d dist",
    "deploy": "git push digitalocean master",
    "v:major": "npm version major && git push --tags",
    "v:minor": "npm version minor && git push --tags",
    "v:patch": "npm version patch && git push --tags",
    "log:all": "node readlogs.js all",
    "log:morgan": "node readlogs.js morgan",
    "log:cron": "node readlogs.js cron"
  },
  "dependencies": {
    "babel-cli": "*",
    "babel-preset-latest": "*",
    "body-parser": "~1.17.1",
    "compression": "^1.6.2",
    "debug": "~2.6.3",
    "dotenv-safe": "^4.0.3",
    "express": "~4.15.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongoose": "^4.9.5",
    "morgan": "~1.8.1",
    "moviedb": "^0.2.8",
    "omdb": "^0.8.0",
    "pm2": "^2.4.5",
    "rarbg": "^0.4.4",
    "rss": "^1.2.2",
    "winston": "^2.3.1",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-eslint": "*",
    "chai": "3.x",
    "chai-http": "3.x",
    "chai-string": "^1.3.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "3.x",
    "mockgoose": "7.x",
    "nodemon": "^1.11.0"
  },
  "engines": {
    "node": "7.x"
  }
}
